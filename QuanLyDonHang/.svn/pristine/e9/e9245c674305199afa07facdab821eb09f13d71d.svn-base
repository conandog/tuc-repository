             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="1170" x:Name="dataUC" Loaded="dataUC_Loaded">
    <Grid>
        <TabControl x:Name="tabData" HorizontalAlignment="Stretch" Height="600" Width="1170" Margin="0,0,0,0" VerticalAlignment="Stretch" SelectionChanged="tabData_SelectionChanged">
            <TabItem x:Name="tabUser" Header="User">
                
            </TabItem>
            <TabItem x:Name="tabNhaThuoc" Header="Nhà Thuốc">
                
            </TabItem>
            <TabItem x:Name="tabCatalogue" Header="Catalogue">
                
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
