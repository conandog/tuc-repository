<Window x:Class="QuanLyDonHang.ManagerWD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Quản Lý Đơn Hàng" Height="736" Width="1280" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100" />
            <RowDefinition Height="636" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" x:Name="stLogo" Margin="1280,100,0,0">
        </StackPanel>
        <StackPanel x:Name="stMainUC" Grid.Row="1" Margin="90,0,0,0">

        </StackPanel>
            <!--Here I want to display  two user controls by switching from button on Top -->
        <StackPanel Grid.Row="1" x:Name="stMainUser" Margin="0,0,0,0" HorizontalAlignment="Left">
        </StackPanel>
    </Grid>
</Window>
