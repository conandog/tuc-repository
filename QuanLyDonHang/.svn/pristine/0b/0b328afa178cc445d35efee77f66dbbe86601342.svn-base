        <Button Name="btExportExcel" Content="Thống kê" HorizontalAlignment="Left" Height="100" Margin="0,400,0,0" VerticalAlignment="Top" Width="90" Click="btExportExcel_Click" RenderTransformOrigin="1.527,0.585" FontSize="16"/>
