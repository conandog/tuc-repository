<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Model="clr-namespace:Model;assembly=Model" x:Name="WindowImportData1" x:Class="QuanLyDonHang.WindowImportData"
        Title="Nhập dữ liệu" Height="600" Width="800" Closed="WindowImportData1_Closed">
    <Grid>
        <DataGrid x:Name="dgShowInfo" CanUserAddRows="False" HorizontalAlignment="Left" Height="478" Margin="50,60,0,0" VerticalAlignment="Top" Width="700
                  " ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" ItemsSource="{Binding}"/>
        <Button x:Name="btImport" Content="Nhập" HorizontalAlignment="Left" Height="29" Margin="50,10,0,0" VerticalAlignment="Top" Width="73" Click="btImport_Click"/>

    </Grid>
</Window>
